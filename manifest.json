{
  "manifest_version": 2,

  "name": "Say goodbye to firewall rules",
  "description": "This extension links you to an proxy that unlock your current url",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "options_ui": {
    "page": "options.html"
  },

  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "storage"
  ]
}
